     ]
    }
   ],
   "source": [
    "arr1=np.array([6,7,8,9,10])\n",
    "y=arr1.view()\n",
    "print(arr1)\n",
    "\n",
    "print(y)\n",
    "print()\n",
    "arr1[2]=30\n",
    "print(arr1)\n",
    "print(y)"
